//保存投屏设置
export const saveScreenSetting = (data) => {
    return axios.request({
        url: `/screen/saveScreenSetting`,
        method: 'post',
        data,
        heads: {
            'Content-Type': 'application/json;charset=UTF-8'
        },
        upload: true
    })
}

